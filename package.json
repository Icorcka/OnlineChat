{
    "name": "fullstack-app",
    "version": "1.0.0",
    "scripts": {
      "install:frontend": "cd client/my-chat-app && npm install",
      "install:backend": "cd server && npm install",
      "start:frontend": "cd client/my-chat-app && npm run start",
      "start:backend": "cd backend && npm run start",
      "start": "npm run start:backend && npm run start:frontend",
      "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\""
    },
    "dependencies": {
    },
    "devDependencies": {
      "concurrently": "^7.0.0"
    }
}
  